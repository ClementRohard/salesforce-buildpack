#!/bin/sh
ENV_DIR=$3

echo "ENV_DIR: $ENV_DIR"
ls $ENV_DIR
export FORCE_COM_PROTOTYPE_USERNAME=$(< "$ENV_DIR/FORCE_COM_PROTOTYPE_USERNAME")
export FORCE_COM_PROTOTYPE_PASSWORD=$(< "$ENV_DIR/FORCE_COM_PROTOTYPE_PASSWORD")
export FORCE_COM_PROTOTYPE_LOGIN_URL=$(< "$ENV_DIR/FORCE_COM_PROTOTYPE_LOGIN_URL")

echo "FORCE_COM_PROTOTYPE_LOGIN_URL: $FORCE_COM_PROTOTYPE_LOGIN_URL"
echo "FORCE_COM_PROTOTYPE_USERNAME: $FORCE_COM_PROTOTYPE_USERNAME"
echo "FORCE_COM_PROTOTYPE_LOGIN_PASSWORD: $FORCE_COM_PROTOTYPE_PASSWORD"
java -jar lib/force-com-buildtool.jar
